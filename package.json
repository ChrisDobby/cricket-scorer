{
  "name": "cricket-scorer",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "start": "npm-run-all --parallel open:src",
    "open:src": "babel-node tools/srcServer.js",
    "lint": "node_modules/.bin/tslint -c tslint.json 'src/**/*.ts{,x}'",
    "test": "jest",
    "test:watch": "yarn run test --watch",
    "prebuild": "npm-run-all clean-dist build:html",
    "postbuild": "babel-node tools/distServer.js",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "build": "npm run prod-build",
    "deploybuild": "npm-run-all clean-dist build:html prod-build",
    "prod-build": "webpack --config ./webpack.config.prod.js --hide-modules --progress",
    "lighthouse": "babel-node tools/lighthouse.js",
    "typecheck": "tsc",
    "prettier": "prettier \"**/*.+(ts|tsx)\"",
    "format": "npm run prettier -- --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChrisDobby/cricket-scorer.git"
  },
  "author": "Chris Dobson",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ChrisDobby/cricket-scorer/issues"
  },
  "homepage": "https://github.com/ChrisDobby/cricket-scorer#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "cheerio": "^1.0.0-rc.2",
    "chrome-launcher": "^0.10.5",
    "colors": "^1.3.3",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "husky": "^1.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.3",
    "jest-fetch-mock": "^2.1.0",
    "jest-junit": "^3.7.0",
    "jest-localstorage-mock": "^2.2.0",
    "lighthouse": "^4.0.0",
    "lint-staged": "^8.1.1",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.16.1",
    "react-testing-library": "^5.4.2",
    "rimraf": "^2.6.2",
    "source-map-loader": "^0.2.3",
    "sw-precache-webpack-plugin": "^0.11.5",
    "ts-jest": "^22.4.4",
    "ts-loader": "^4.2.0",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.8.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-language-service": "^0.9.9",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^3.5.1",
    "typescript": "^2.8.3",
    "webpack": "^4.6.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.1",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "dependencies": {
    "@material-ui/core": "^3.4.0",
    "@material-ui/icons": "^3.0.1",
    "@types/auth0-js": "^8.11.6",
    "@types/dateformat": "^1.0.1",
    "@types/jest": "^22.2.3",
    "@types/react": "^16.7.0",
    "@types/react-autosuggest": "^9.3.6",
    "@types/react-dom": "^16.0.5",
    "@types/react-fontawesome": "^1.6.4",
    "@types/react-router-dom": "^4.2.6",
    "@types/react-swipeable-views": "^0.12.2",
    "@types/socket.io-client": "^1.4.32",
    "auth0-js": "^9.13.2",
    "babel-jest": "^22.4.3",
    "compression": "^1.7.3",
    "css-loader": "^0.28.11",
    "dateformat": "^3.0.3",
    "express": "^4.16.3",
    "file-loader": "^1.1.11",
    "mobx": "^4.2.1",
    "mobx-react": "^5.1.2",
    "open": "^6.0.0",
    "react": "^16.8.0",
    "react-autosuggest": "^9.4.3",
    "react-dom": "^16.8.0",
    "react-router-dom": "^4.2.2",
    "react-swipeable-views": "^0.13.0",
    "react-transition-group": "^2.5.2",
    "socket.io-client": "^2.1.1",
    "style-loader": "^0.21.0"
  }
}